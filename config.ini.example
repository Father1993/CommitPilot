# CommitPilot - Пример конфигурации
# Скопируйте этот файл в config.ini и настройте под себя

[DEFAULT]
# Выберите провайдера AI: aitunnel (по умолчанию), huggingface или openai
api_provider = aitunnel

# AITUNNEL API настройки (рекомендуется)
# Токен можно также указать в файле .env как AI_TUNNEL=sk-aitunnel-xxx
aitunnel_token = 
aitunnel_base_url = https://api.aitunnel.ru/v1/
aitunnel_model = gpt-4.1

# Hugging Face API настройки
huggingface_token = 

# OpenAI API настройки
openai_token = 

# Ветка по умолчанию для git push
branch = master

# Максимальный размер diff для отправки в AI API
max_diff_size = 7000
